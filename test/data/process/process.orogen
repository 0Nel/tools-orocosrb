name 'process'
version '1.0'

typekit do
    load "simple_types.h"
end

task_context 'Test' do
    property "prop1", "/process/Simple"
    property "prop2", "int"
    property 'prop3', 'string'

    attribute "att1", "/process/Simple"
    attribute "att2", "int"
    attribute 'att3', 'string'
end

deployment "process" do
    test = task('process_Test', 'Test').
        periodic(0.1).
        start
end
